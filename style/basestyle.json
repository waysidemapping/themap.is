        },
        {
            "id": "station-outline",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "line",
            "filter": [
                "all",
                [
                    "any",
                    ["==", ["get", "public_transport"], "station"],
                    ["==", ["get", "aeroway"], "aerodrome"],
                    ["==", ["get", "railway"], "station"]
                ],
                ["!", ["has", "building"]]
            ],
            "layout": {
