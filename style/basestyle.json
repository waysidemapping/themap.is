{
    "version": 8,
    "name": "Rusic Tiles Demo Style",
    "glyphs": "https://tiles.openstreetmap.us/fonts/{fontstack}/{range}.pbf",
    "sources": {
       "beefsteak": {
            "type": "vector",
            "url": "http://159.69.74.234/beefsteak"
        }
    },
    "sprite": "/style/sprites/themapis",
    "layers": [
        {
            "id": "background",
            "type": "background",
            "paint": {
                "background-color": "#fff"
            }
        },
        {
            "id": "aboriginal-lands-fill",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "fill",
            "filter": ["==", ["get", "boundary"], "aboriginal_lands"],
            "paint": {
                "fill-color": "#FFF8F2"
            },
            "maxzoom": 12
        },
        {
            "id": "developed-fill",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "fill",
            "filter": ["in", ["get", "landuse"], ["literal", ["residential", "commercial", "industrial", "retail", "brownfield", "garages", "railway"]]],
            "paint": {
                "fill-color": "#F6F6F6"
            }
        },
        {
            "id": "park-fill",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "fill",
            "filter": [
                "all",
                [
                    "any",
                    ["in", ["get", "boundary"], ["literal", ["protected_area"]]],
                    ["in", ["get", "leisure"], ["literal", ["nature_reserve", "park"]]]
                ],
                ["!", ["==", ["get", "protected_area"], "national_park"]],
                ["!", ["==", ["get", "maritime"], "yes"]]
            ],
            "paint": {
                "fill-color": "#ECFAE9"
            },
            "maxzoom": 12
        },
        {
            "id": "national-park-fill",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "fill",
            "filter": [
                "all",
                ["==", ["get", "protected_area"], "national_park"],
                ["!", ["==", ["get", "maritime"], "yes"]]
            ],
            "paint": {
                "fill-color": "#DAEDD5"
            },
            "maxzoom": 12
        },
        {
            "id": "military-fill",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "fill",
            "filter": [
                "any",
                ["==", ["get", "landuse"], "military"],
                ["==", ["get", "military"], "base"]
            ],
            "paint": {
                "fill-color": "#FDEFED"
            },
            "maxzoom": 12
        },
        {
            "id": "education-fill",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "fill",
            "filter": [
                "all",
                ["!", ["has", "building"]],
                [
                    "any",
                    ["has", "education"],
                    ["in", ["get", "amenity"], ["literal", ["school", "college", "university"]]],
                    ["in", ["get", "landuse"], ["literal", ["education"]]]
                ]
            ],
            "paint": {
                "fill-color": "#FFF9DB"
            },
            "maxzoom": 12
        },
        {
            "id": "station-fill",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "fill",
            "filter": [
                "all",
                [
                    "any",
                    ["==", ["get", "public_transport"], "station"],
                    ["==", ["get", "aeroway"], "aerodrome"],
                    ["==", ["get", "railway"], "station"]
                ],
                ["!", ["has", "building"]]
            ],
            "paint": {
                "fill-color": "#E3E9FA"
            },
            "maxzoom": 12
        },
        {
            "id": "water-fill",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "fill",
            "filter": ["in", ["get", "natural"], ["literal", ["coastline", "water"]]],
            "paint": {
                "fill-color": "#D9F0FF"
            }
        },
        {
            "id": "pier-fill",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "fill",
            "filter": ["in", ["get", "man_made"], ["literal", ["pier", "bridge"]]],
            "paint": {
                "fill-opacity": 0.4,
                "fill-color": "#d0d0d0"
            }
        },
        {
            "id": "maritime-park-fill",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "fill",
            "filter": [
                "all",
                ["==", ["get", "maritime"], "yes"],
                [
                    "any",
                    ["in", ["get", "boundary"], ["literal", ["protected_area"]]],
                    ["in", ["get", "leisure"], ["literal", ["nature_reserve", "park"]]]
                ]
            ],
            "paint": {
                "fill-color": "#CEECED"
            },
            "maxzoom": 12
        },
        {
            "id": "barrier-fill",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "fill",
            "filter": [
                "any",
                ["has", "barrier"],
                ["in", ["get", "man_made"], ["literal", ["breakwater", "dyke", "groyne"]]],
                ["in", ["get", "waterway"], ["literal", ["dam", "weir"]]]
            ],
            "paint": {
                "fill-color": "#ddd"
            }
        },
        {
            "id": "parking-fill",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "fill",
            "filter": [
                "all",
                ["!", ["has", "building"]],
                ["in", ["get", "amenity"], ["literal", ["parking"]]]
            ],
            "paint": {
                "fill-color": "#f0f0f0"
            },
            "minzoom": 12
        },
        {
            "id": "coastline",
            "source": "beefsteak",
            "source-layer": "line",
            "type": "line",
            "filter": [
                "all",
                ["==", ["get", "natural"], "coastline"],
                ["!", ["==", ["get", "maritime"], "yes"]
            ]],
            "layout": {
                "line-join": "round"
            },
            "paint": {
                "line-color": "#BFD3E0",
                "line-width": 0.4
            }
        },
        {
            "id": "aboriginal_lands-outline",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "line",
            "filter": ["==", ["get", "boundary"], "aboriginal_lands"],
            "layout": {
                "line-join": "round"
            },
            "paint": {
                "line-color": "#FFF8F2",
                "line-width": 3.6,
                "line-offset": 1.8
            },
            "minzoom": 12
        },
        {
            "id": "aboriginal_lands-outline2",
