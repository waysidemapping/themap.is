            "type": "line",
            "filter":  ["in", ["get", "power"], ["literal", ["line", "minor_line", "cable"]]],
            "paint": {
                "line-opacity": [
                    "case",
                    ["in", ["get", "location"], ["literal", ["underground", "underwater", "indoor"]]], 0.6,
