            "type": "line",
            "filter": [
                "all",
                ["!", ["has", "building"]],
                [
                    "any",
                    ["has", "education"],
                    ["in", ["get", "amenity"], ["literal", ["school", "college", "university"]]],
                    ["in", ["get", "landuse"], ["literal", ["education"]]]
                ]
            ],
            "layout": {
