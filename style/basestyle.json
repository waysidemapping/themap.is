                "line-width": 0.4
            }
        },
        {
            "id": "park-outline",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "line",
            "filter": ["all", ["!", ["==", ["get", "maritime"], "yes"]], ["any", ["in", ["get", "boundary"], ["literal", ["protected_area"]]], ["in", ["get", "leisure"], ["literal", ["nature_reserve", "park"]]]]],
            "layout": {
                "line-join": "round"
            },
            "paint": {
                "line-color": "#ECFAE9",
                "line-width": 3.6,
                "line-offset": 1.8
            },
            "minzoom": 12
        },
        {
            "id": "park-outline2",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "line",
            "filter": ["all", ["!", ["==", ["get", "maritime"], "yes"]], ["any", ["in", ["get", "boundary"], ["literal", ["protected_area"]]], ["in", ["get", "leisure"], ["literal", ["nature_reserve", "park"]]]]],
            "layout": {
                "line-join": "round"
            },
            "paint": {
                "line-color": "#B5D4AF",
                "line-width": 0.4
            }
        },
        {
            "id": "maritime-park-outline",
            "source": "beefsteak",
            "source-layer": "area",
            "type": "line",
            "filter": ["all", ["==", ["get", "maritime"], "yes"], ["any", ["in", ["get", "boundary"], ["literal", ["protected_area"]]], ["in", ["get", "leisure"], ["literal", ["nature_reserve", "park"]]]]],
            "layout": {
                "line-join": "round"
            },
            "paint": {
                "line-color": "#CEECED",
                "line-width": 3.6,
                "line-offset": 1.8
            },
