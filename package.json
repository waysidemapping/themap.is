        "maplibre-gl": "~5.18.0",
        "potpack": "~2.1.0"
    },
    "devDependencies": {
        "chalk": "^5.2.0",
        "svg-path-parse": "^1.1.3",
        "xmlbuilder2": "^4.0.3"
    }
}
